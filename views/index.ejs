<!DOCTYPE html>
<html ng-app="projectRtc">

<head>
    <title>
        <% title %>
    </title>
    <base href="/" />
    <!-- css -->
    <!-- <link href='https://fonts.googleapis.com/css?family=Roboto:400,700,700italic' rel='stylesheet' type='text/css'> -->
    <link rel='stylesheet' href='/fonts/roboto.css' type='text/css'>

    <link rel='stylesheet' href='/stylesheets/style.css' />

    <!-- <link rel='stylesheet' href='/stylesheets/style-headerNoLogo.css' /> -->
    <link rel='stylesheet' href='/stylesheets/style-remotestream.css' />
    <link rel='stylesheet' href='/stylesheets/style-chat.css' />
    <link rel='stylesheet' href='/stylesheets/style-userinfo.css' />
    <link rel='stylesheet' href='/stylesheets/style-chart.css' />
    <link rel='stylesheet' href='/stylesheets/style-alerts-bottom.css' />
    <link rel='stylesheet' href='/stylesheets/nav.css' />
    <link rel='stylesheet' href='/stylesheets/button.css' />

    <!-- custom css -->
    <link rel='stylesheet' href='/custom/<%= idCompanyAndUser.company_shortname %>/header.css' />
    <!-- login logo -->
    <!-- <link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css"> -->
    <link rel="stylesheet" href="/fonts/awesome/css/font-awesome.min.css">

    <!-- sockets for signaling -->
    <script type="text/javascript" src="/socket.io/socket.io.js" defer></script>

    <style>
        #map {
            width: 500px;
            height: 400px;
        }
    </style>
    <script src='//maps.googleapis.com/maps/api/js'></script>
    <script src="../javascripts/lib/angular.min.js" defer></script>


    <!-- charts -->
    <script type="text/javascript" src="../javascripts/lib/smoothie.js" defer></script>
    <script type="text/javascript" src="../javascripts/scriptChart.js" defer></script>

    <!-- recording -->
    <script type="text/javascript" src="../javascripts/scriptRecording.js" defer></script>

    <!-- server -->
    <script type="text/javascript" src="javascripts/lib/adapter.js" defer></script>

    <script type="text/javascript" src="javascripts/rtcClient.js" defer></script>
    <script type="text/javascript" src="javascripts/newapp.js" defer></script>

</head>

<body ng-controller="RemoteStreamsController as rtc">
    <div id="header" class="card">

   
                    <%= include headerNoLogo %>

    </div>

    <div id="container">
     

    </div>
    <!-- end container -->

    <div class="footer">

        <%= include footer %>
    </div>
</body>

</html>